           case 15:
           {
               Semester_Exams();
               break;
           }*/
       }
   }
   while(choice != 0);
}

void Print_Header()
{
    printf("Sun  Mon  Tue  Wed  Thu  Fri  Sat\n");
}

void Print_Month(int startDay, int numDays)
{
    int day, col;
    
    for(col = 0; col < startDay; col++)
    {
        printf("     ");
    }
    
    for(day = 1; day <= numDays; day++)
    {
        printf("%3d  ", day);
        if(++col > 6)
        {
            col = 0;
            printf("\n");
        }
    }
    
    if(col != 0)
    {
        printf("\n");
    }
}

void September_2023()
{
    int startDay = 5;
    int numDays = 30;
    Print_Header();
    Print_Month(startDay, numDays);
}

void October_2023()
{
    int startDay = 0;
    int numDays = 31;
    Print_Header();
    Print_Month(startDay, numDays);
}

void November_2023()
{
    int startDay = 3;
    int numDays = 30;
    Print_Header();
    Print_Month(startDay, numDays);
}

void December_2023()
{
    int startDay = 5;
    int numDays = 31;
    Print_Header();
    Print_Month(startDay, numDays);
}

