       printf("\n14. CAT exams");
       printf("\n15. Semester Exams\n");
       
       printf("\nEnter your Choice: ");
       scanf("%d",&choice);
       
       switch(choice)
       {
           case 1:
           {
               September_2023();
               break;
           }
           case 2:
           {
               October_2023();
               break;
           }
           case 3:
           {
               November_2023();
               break;
           }
           case 4:
